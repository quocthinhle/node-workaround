{
    "env": {
        "commonjs": true,
        "es2021": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["warn", 4],
        "class-methods-use-this": "off",
        "no-param-reassign": "off"
    }
}
